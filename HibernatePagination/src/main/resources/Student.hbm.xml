<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
    <class name="com.garg.model.Student" table="STUDENT">
        <id name="id" type="int" column="id" >
            <generator class="increment"/>
        </id>
        <property name="studentname" not-null="true" >
            <column name="studentname" />
        </property>
        <property name="email">
            <column name="email"/>
        </property>
       
        
    </class>
</hibernate-mapping>